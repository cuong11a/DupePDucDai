btnStroke.Thickness = 2
btnStroke.Color = Color3.fromRGB(255, 100, 255)
btnStroke.Transparency = 0.3

dupeButton.MouseEnter:Connect(function()
	TweenService:Create(dupeButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
		Size = UDim2.new(0.9, 0, 0, 50)
	}):Play()
	btnStroke.Transparency = 0
end)
dupeButton.MouseLeave:Connect(function()
	TweenService:Create(dupeButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
		Size = UDim2.new(0.85, 0, 0, 44)
	}):Play()
	btnStroke.Transparency = 0.3
end)

-- ðŸ¾ NÃºt Spawner
local spawnerButton = dupeButton:Clone()
