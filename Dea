-- ðŸ”˜ NÃºt Dupe
local dupeButton = Instance.new("TextButton", mainFrame)
dupeButton.Size = UDim2.new(0.85, 0, 0, 44)
dupeButton.Position = UDim2.new(0.075, 0, 0.45, 0)
